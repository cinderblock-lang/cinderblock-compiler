namespace Metadata {
  struct CodeLocation {
    file_name: string;
    start_line: uint;
    start_column: uint;
    end_line: uint;
    end_column: uint;
  }

  fn AsString(self: CodeLocation): string {
    return "\nFile: "
      .concat(self.file_name)
      .concat("\nLine: ")
      .concat();
  }
}